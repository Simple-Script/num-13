# Check if the first two text spots in a file contain '13'

def check_file_content(file_path):
    with open(file_path, 'r') as file:
        content = file.read().split()
        if len(content) >= 2 and content[0] == '13' and content[1] == '13':
            return 'yes'
        else:
            return 'no'

file_path = 'your_file.txt'  # Replace with your file path
result = check_file_content(file_path)
print(result)
